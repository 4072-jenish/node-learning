<%- include("header") %>
<div class="page-wrapper">
 <tbody>
    <% user.forEach((user, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= user.firstName %></td>
          <td><%= user.lastName %></td>
          <td><%= user.email %></td>
          <td><%= user.hobbies %></td>
          <td><%= user.gender %></td>
          <td><img src="<%= user.image %>" alt="User Image" width="50" /></td>
          <td><%= user.contactNo %></td>
          <td><a href="/users/edit-user/<%= user._id %>">Edit</a></td>
          <td><a href="/users/delete-user/<%= user._id %>">Delete</a></td>
        </tr>
      <% }) %>
</tbody>
</div>

<%- include("footer") %>
